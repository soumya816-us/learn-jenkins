pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                sh 'echo Hello-World by Build'
            }
        }
        stage('Test') {
            steps {
                sh 'echo Hello-World by Test'
            }
        }
        stage('Deploy') {
            steps {
                sh 'echo Hello-World by Deploy'
            }
        }
    }
}